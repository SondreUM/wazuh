{
    "id": 1,
    "name": "sedexp-udev",
    "description": "Detects changes to udev rules, used by sedexp malware",
    "before": 5,
    "after": 5,
    "conditions": [
        {
            "startswith": "File '/etc/udev/rules.d/"
        },
        {
            "contains": "sedexp"
        }
    ],
    "ext": {
        "tag": "linux",
        "mitre": "T1546.017"
    }
}